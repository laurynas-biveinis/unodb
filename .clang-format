---
Standard: c++20
BasedOnStyle: Google
IncludeBlocks: Regroup
IncludeCategories:
  - Regex: '^"global\.hpp"$'
    Priority: -1
  - Regex: '^<[a-z_]+>$'
    Priority: 2
  - Regex: '^<.*>$'
    Priority: 3
  - Regex: '^".*"$'
    Priority: 4
AttributeMacros:
  - UNODB_DETAIL_UNUSED
  - UNODB_DETAIL_USED_IN_DEBUG
  - UNODB_DETAIL_LIFETIMEBOUND
  - UNODB_DETAIL_NOINLINE
  - UNODB_DETAIL_C_STRING_ARG
  - UNODB_DETAIL_HEADER_NOINLINE
StatementAttributeLikeMacros:
  - UNODB_DETAIL_RELEASE_CONST
